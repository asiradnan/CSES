//https://cses.fi/problemset/task/1070/
#include<iostream>
using namespace std;
using ll=long long;
int main()
{
    ios::sync_with_stdio(false);
    cin.tie(0);
    cout.tie(0);
	int n;
	cin>>n;
	if (n==2 || n==3) cout<<"NO SOLUTION\n";
	else{
		for (int i=2;i<=n;i+=2) cout<<i<<' ';
		for (int i=1;i<=n;i+=2) cout<<i<<' ';
		cout<<endl;
	}
}
